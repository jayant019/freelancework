            <stringProp name="script">public String CreateInputString(String FilePath, int noOfSymbols, int isEncodingRequired){
//required imports below
import org.apache.commons.io.FileUtils; 
import java.util.*;
import java.net.URLEncoder;
import java.net.URLDecoder;

List lines = FileUtils.readLines(new File(FilePath));  // read file into lines array   -  Specify the full path of file here

//log.info(randomLine);

StringBuilder finalString= new StringBuilder();
for(int i=1;i&lt;=noOfSymbols;i++) {
	int random = new Random().nextInt(lines.size()); // get random line number
	String randomLine = lines.get(random); // get random line/row from CSV
	randomLine=randomLine.replace(&quot;,&quot;,&quot;&quot;);	// remove unwanted commas in the String value
